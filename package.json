{
  "name": "dash-live",
  "version": "2.10.0",
  "description": "A simulated MPEG DASH service",
  "license": "Apache-2.0",
  "repository": "github:asrashley/dash-live",
  "browser": "static/js/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "lint": "npm run lint:compat && npm run lint:esm",
    "lint:compat": "jshint ./static/js/legacy/*.js static/js/compat/*.js",
    "lint:esm": "eslint ./static/js/*.js ./static/js/spa/**/*.js",
    "postinstall": "patch-package",
    "update:prod": "copy-files-from-to --config deploy/copy-to-prod.json"
  },
  "private": true,
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@preact/signals": "^1.3.0",
    "@preact/signals-core": "^1.8.0",
    "eslint": "^8.57.1",
    "eslint-config-preact": "^1.5.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.1",
    "jshint": "^2.13.6"
  },
  "dependencies": {
    "bootstrap-icons": "^1.11.3",
    "copy-files-from-to": "^3.11.0",
    "htm": "^3.1.1",
    "jquery": "^3.7.1",
    "linkstate": "^2.0.1",
    "patch-package": "^8.0.0",
    "preact": "^10.23.1",
    "regexparam": "^3.0.0",
    "temporal-polyfill": "^0.2.5",
    "wouter-preact": "^3.3.5"
  }
}
